{
  "name": "rusty-bot",
  "version": "2.1.0",
  "description": "A discord bot for idiots",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "deploy": "npm run build && gcloud app deploy",
    "dev": "nodemon src/index.ts && npm run copy-files",
    "build": "npm run validate && genversion src/utilities/version.ts && tsc && npm run copy-files",
    "copy-files": "copyfiles \"src/assets/*\" \"dist/assets\" --flat",
    "validate": "npm run validate-activity-messages",
    "validate-activity-messages": "node ./scripts/activity-message-validation.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr1upMachine/rusty-bot.git"
  },
  "keywords": [
    "discord",
    "rusty"
  ],
  "contributors": [
    "Sean Hale",
    "Ty Simpson"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mr1upMachine/rusty-bot/issues"
  },
  "homepage": "https://github.com/Mr1upMachine/rusty-bot#readme",
  "engines": {
    "node": ">=16.8.0",
    "npm": ">=7.21.0"
  },
  "dependencies": {
    "@discordjs/builders": "0.6.0",
    "@discordjs/rest": "0.1.0-canary.0",
    "axios": "0.21.1",
    "cron": "1.8.2",
    "discord-api-types": "0.22.0",
    "discord.js": "13.1.0",
    "dotenv": "10.0.0",
    "firebase-admin": "9.11.1",
    "mersenne-twister": "1.1.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/mersenne-twister": "1.1.2",
    "@types/node": "16.7.2",
    "@types/node-fetch": "2.5.12",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/eslint-plugin-tslint": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.0.8",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "genversion": "3.0.1",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  }
}
